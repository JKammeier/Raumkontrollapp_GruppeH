<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:animateLayoutChanges="true"
    android:clickable="true"
    android:focusable="true"
    tools:context=".AusstattungsDetailFragment">

    <TextView
        android:id="@+id/textview_second"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView_01"
        android:layout_width="242dp"
        android:layout_height="82dp"
        android:layout_marginTop="120dp"
        android:autoText="false"
        android:background="@color/teal_200"
        android:backgroundTint="@color/teal_200"
        android:clickable="false"
        android:contextClickable="false"
        android:text="@string/geben_sie_bitte_die_raumnummer_ein_und_dr_cken_sie_unten_um_die_raumeigenschaften_zu_bearbeiten"
        android:textColor="@color/black"
        android:textSize="16sp"
        android:visibility="visible"
        app:layout_constraintBottom_toTopOf="@+id/textView4"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.497"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0"
        tools:visibility="visible" />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="157dp"
        android:layout_height="35dp"
        android:text="Raumnummer:"
        android:textColor="@color/black"
        app:barrierAllowsGoneWidgets="true"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.429" />

    <Button
        android:id="@+id/button_00"
        android:layout_width="188dp"
        android:layout_height="95dp"
        android:layout_marginBottom="204dp"
        android:backgroundTint="@color/teal_200"
        android:clickable="true"
        android:focusable="true"
        android:keepScreenOn="true"
        android:text="@string/bearbeiten"
        android:textColor="@color/black"
        android:textSize="14sp"
        android:visibility="visible"
        app:icon="@android:drawable/ic_menu_edit"
        app:iconGravity="textTop"
        app:iconTint="@color/black"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.497"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView4"
        app:layout_constraintVertical_bias="0.142"
        tools:visibility="visible" />

</androidx.constraintlayout.widget.ConstraintLayout>


